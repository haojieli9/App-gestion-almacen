<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-sdk tools:overrideLibrary="com.google.zxing.client.android" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:hardwareAccelerated="true"
        android:icon="@drawable/png_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/png_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoMultimedia"
        tools:targetApi="31">
        <activity
            android:name=".ui.productos.details.DetailProduct"
            android:exported="false" />
        <activity
            android:name=".ui.almacen.details.DetailStock"
            android:exported="false" />
        <activity
            android:name=".ui.almacen.details.DetailsAlmacen"
            android:exported="false" />
        <activity
            android:name=".ui.productos.ActivityProductos"
            android:exported="false" />
        <activity
            android:name=".ui.register.RegValidar"
            android:exported="false" />
        <activity
            android:name=".ui.register.Registrar"
            android:exported="false" />
        <activity
            android:name=".test.Politica_web"
            android:exported="false" />
        <activity
            android:name=".test.ActivityPruebas"
            android:exported="false" />
        <activity
            android:name=".test.FormAlmacen"
            android:exported="false" />
        <activity
            android:name=".ui.splash.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.almacen.ActivityAlmacen"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="portrait"
            tools:replace="screenOrientation" />
    </application>

</manifest>